
  <title></title>
  <% if (user) { %>
      <h1>Hello <%= user.email %> :)</h1>
    <form method="post" action="/logout" >
      <button>Logout</button>
    </form>
  <% } else { %>
      <h1>Hello :)</h1>
      <form action="/register" method="post">
        <button type="submit">Register</button>
      </form>
      <form action="/login" method="post">
        <button type="submit">Login</button>
      </form>
  <% } %>
